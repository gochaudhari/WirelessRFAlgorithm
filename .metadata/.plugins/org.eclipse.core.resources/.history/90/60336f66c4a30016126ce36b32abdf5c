#include <Common.h>

char *ShiftRegister(uint8_t *ShiftBuffer, int lengthOfBuffer, ShiftDirection shiftDirection, int shiftCount)
{
	int counter = 0;
	uint8_t localCharHandle;
	uint8_t localBuffer[lengthOfBuffer] = {0};

	for(counter = 0; counter < lengthOfBuffer; counter++)
	{
		if(counter == 0)
		{
			localBuffer
		}
		localCharHandle = ShiftBuffer[counter] << shiftCount;

	}
}

